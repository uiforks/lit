{
  "$schema": "https://raw.githubusercontent.com/Polymer/tachometer/master/config.schema.json",
  "root": "../../../../",
  "sampleSize": 50,
  "timeout": 0,
  "benchmarks": [
    {
      "url": "index.html",
      "browser": {
        "name": "chrome",
        "headless": true
      },
      "measurement": [
        {
          "mode": "performance",
          "entryName": "render"
        },
        {
          "mode": "performance",
