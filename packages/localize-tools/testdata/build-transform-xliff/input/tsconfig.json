{
  "compilerOptions": {
    "target": "es2018",
    "strict": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "rootDir": "./",
    "outDir": "tsout",
    "types": []
  },
  "include": ["**/*.ts"]
}
