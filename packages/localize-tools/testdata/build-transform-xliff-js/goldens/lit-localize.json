{
  "$schema": "../../../config.schema.json",
  "sourceLocale": "en",
  "targetLocales": ["es-419", "zh_CN"],
  "inputFiles": ["**/*.js"],
  "output": {
    "mode": "transform",
    "outputDir": "locales",
    "localeCodesModule": "locale-codes.js"
  },
  "interchange": {
    "format": "xliff",
    "xliffDir": "xliff/",
    "placeholderStyle": "ph"
  }
}
